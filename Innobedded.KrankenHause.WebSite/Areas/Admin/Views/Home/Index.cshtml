@section js{


    <script type="text/javascript">

        $('#fach').click(function () {
            show('Fach')
        });

        function show(param) {
            //   alert("HASAN");
            var Url = "/admin/";
            switch (param) {

                case 'Fach':
                    Url = Url + "faches/create"
                    break;

                default:
                    Url = Url + "faches/create"

            }

            $(document).ready(function () {
                $.get(Url, { input: param }, function (data) {
                    $('#AllContent').html(data);
                });

            });
        }
    </script>


